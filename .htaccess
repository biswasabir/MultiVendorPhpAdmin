Header set Access-Control-Allow-Headers "Content-Type, X-CSRF-TOKEN"
SetEnvIf Authorization "(.*)" HTTP_AUTHORIZATION=$1

RewriteCond %{HTTP:Authorization} ^(.*)
RewriteRule .* - [e=HTTP_AUTHORIZATION:%1]

Options -Indexes

# force ssl
RewriteCond     %{SERVER_PORT} ^80$
RewriteRule     ^(.*)$ https://%{SERVER_NAME}%{REQUEST_URI} [L,R]

Header always edit Set-Cookie (.*) "$1; HTTPOnly"
Header always edit Set-Cookie (.*) "$1; Secure"

<IfModule mime_module>
  AddType text/html .php
</IfModule>

redirectMatch 301 /refer https://play.google.com/store/apps/details?id=wrteam.multivendor.shop

redirectMatch 301 /itemdetail https://play.google.com/store/apps/details?id=wrteam.multivendor.shop

# php -- BEGIN cPanel-generated handler, do not edit
# Set the “ea-php73” package as the default “PHP” programming language.
<IfModule mime_module>
  AddHandler application/x-httpd-ea-php73 .php .php7 .phtml
</IfModule>
# php -- END cPanel-generated handler, do not edit


